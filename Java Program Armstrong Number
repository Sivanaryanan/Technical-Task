import java.util.Scanner;
class Armstrong
{	
public static void main(String ar[])
{
int n,s=0,r,n1;
Scanner cr=new Scanner(System.in);
System.out.println("enter Armstrong num");
n=cr.nextInt();
n1=n;
while(n>0)
{
r=n%10;
s=s+(r*r*r);
n=n/10;
}
if(n1==s)
System.out.println("Armstrong num");
else
System.out.println("not Armstrong num");
}
}
